{
  "name": "car-simulator",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href 'https://alexgervas.github.io/car-simulator/' && node copy-404.js && node rename-browser-to-docs.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless": "ng test --karma-config karma.conf.ci.cjs --watch=false --browsers=ChromeHeadlessNoSandbox --code-coverage",
    "start-bot": "node bot.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss,json}\"",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "bcrypt": "^6.0.0",
    "cannon-es": "^0.20.0",
    "cannon-es-debugger": "^1.0.0",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "rxjs": "~7.8.0",
    "telegraf": "^4.16.3",
    "three": "^0.173.0",
    "three-fbx-loader": "^1.0.3",
    "tslib": "^2.3.0",
    "validator": "^13.15.15",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@eslint/js": "^9.39.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.4",
    "@types/three": "^0.173.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "fs-extra": "^11.3.0",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-webpack": "^5.0.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.54.0"
  }
}
